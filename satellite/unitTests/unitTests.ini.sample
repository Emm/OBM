[backupEntity]

os-server = 127.0.0.1

# User entity login
user-login = test01@aliasource.fr
# Mailshare entity name
mailshare-name = mailshare00@aliasource.fr
# Send report copy to this emails too (separated by comma, no mail send if empty)
send-report = 
# Push archive on domain FTP backup (default: false)
push-ftp = true

# Backup entity
test-backup-entity = true
# Backup entity with invalid request content (not XML)
test-backup-entity-invalid-request-content = true
# Backup entity with no request content (no ICS and no VCARD)
test-backup-entity-no-request-content = true
# Get available backup file list
test-list-available-backup = true
# Restore entity with invalid file name in request content (not
# exist/readable/...)
test-restore-invalid-file = true
# Resore ICS only
test-restore-ics = true
# Resore VCARD only
test-restore-vcard = true
# Resore mailbox only
test-restore-mailbox = true
# Retrieve backup from backup FTP server
test-retrieve-backup = false
